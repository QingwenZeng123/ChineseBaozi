                                     | Exp5
      Exp5                      = Exp5 ("*" | "/" | "%") Exp6                                                   -- two
                                     | Exp6
      Exp6                      = Exp7 "**" Exp6                                                                   -- two
                                     | Exp7
      Exp7                      = bool
                                     | id
                                     | num
                                     | "(" Exp ")"                                                                            --parenth
                                     | stringlit
                                     | charlit
                                     
